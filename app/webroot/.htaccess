<IfModule mod_rewrite.c>

	RewriteEngine On

    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteRule ^(.*)$ index.php?url=$1 [QSA,L]
	
	#RewriteCond %{SERVER_PORT} 80 
	#RewriteRule ^(.*)$ https://www.luccaantiques.com/$1 [R,L]
	
	RewriteCond %{HTTP_HOST} ^luccaantiques\.com$
	RewriteRule (.*) http://www.luccaantiques.com/$1 [R=301,L]

</IfModule>